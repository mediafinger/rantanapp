
<article>
  FORM
  <?r x = 98 ?>
  <i>#{x}</i>

  #{ form_with(model: team) do |form| }
    #{ render partial: "shared_partials/form_validation_errors", locals: { record: team } }

    <fieldset>
      <h3><legend>#{ form_legend }</legend></h3>

      <div>
        #{ form.label :name }
        #{ form.text_field :name }
      </div>

      <div>
        #{ form.submit }
      </div>
    </fieldset>
  <?r end ?>
</article>
